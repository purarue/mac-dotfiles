#!/usr/bin/env python3

import os
import webbrowser
import xml.etree.ElementTree as ET

jenkins_config_path = os.path.join(os.path.join(os.environ["HOME"], ".jenkins"), "jenkins.model.JenkinsLocationConfiguration.xml")
tree = ET.parse(jenkins_config_path)
jenkins_url = next(tree.iter("jenkinsUrl")).text
webbrowser.open_new_tab(jenkins_url)
